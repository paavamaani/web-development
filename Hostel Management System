<script>

function Validate() {
	
	var val1 = document.forms["form"]["fname"].value;
	
	if(val1 == "") {
	
		alert("Enter First Name");
		return false;
		
	}	
	
	var val2 = document.forms["form"]["lname"].value;
	
	if(val2 == "") {
	
		alert("Enter Last Name");
		return false;
		
	}
	
	var val3 = document.forms["form"]["email"].value;
	
	if(val3 == "") {
	
		alert("Enter Email");
		return false;
		
	}
	
	var val4 = document.forms["form"]["password"].value;
	
	if(val4 == "") {
		
		alert("Password is required");
		return false;
	
	}
	
	var val5 = document.forms["form"]["confirmpassword"].value;
	
	if(val5 == "") {
	
		alert("Please confirm your Password");
		return false;
	}	
	
	var val6 = document.forms["form"]["gender"].value;
	
	if(val6 == "") {
	
		alert("Please confirm your Gender");
		return false;
	}	
	
	var val7 = document.forms["form"]["code"].value;
	
	if(val7 == "") {
	
		alert("Please enter country code");
		return false;
	}	
	
	var val8 = document.forms["form"]["phone"].value;
	
	if(val8 == "") {
	
		alert("Please enter mobile number");
		return false;
	}	
	
	var val9 = document.forms["form"]["bday"].value;
	
	if(val9 == "") {
	
		alert("Please enter birth date");
		return false;
	}
	
	var val10 = document.forms["form"]["place"].value;
	
	if(val10 == "") {
	
		alert("Please enter place");
		return false;
	}
	
	var val11 = document.forms["form"]["seater"].value;
	
	if(val11 == "select") {
	
		alert("Please select seater");
		return false;
	}
	
	var val12 = document.forms["form"]["duration"].value;
	
	if(val12 == "select") {
	
		alert("Please select duration");
		return false;
	}
	
	if(val4==val5) {
	
		alert("Your response has been recieved")
		return true;
	}
	else {
	
		alert("Password and Confirm Password don't match");
		return false;

	}
}

</script>

